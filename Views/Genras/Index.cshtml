@model IEnumerable<labb3.Models.Genra>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="rightcolumn">
        <h1 class="txt">LIST OF CD NAMES</h1>

        @foreach (var item in Model)
        {
            <div class="column">
                <div class="card">
                    <div class="container">
                        <p>ARTIST NAME: @item.Artist.Name</p>
                        <p>CD NAME: @item.Name</p>
                        <a asp-action="Details" asp-route-id="@item.Id">Read More >></a>
                    </div>
                </div>
            </div>

        }
    </div>

